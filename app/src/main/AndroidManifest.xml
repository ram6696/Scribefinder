            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".internetConnection" />
        <activity
            android:name=".StudyMaterials.BANotes.BA1stSemEnglsih"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudyMaterials.BANotes.BA1stSemEPH"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudyMaterials.BANotes.BA1stSemHistory"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudyMaterials.BANotes.BA2ndSemHistory"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudyMaterials.BANotes.BA4thSemEconomics"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".StudyMaterials.BANotes.BA5thSemCorporate"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".StudyMaterials.BANotes.BA5thSemRural"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>