<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ourapps.scribefinder">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_logo"
        android:supportsRtl="true">
        <activity
            android:name=".LogoSplashScreen"
            android:theme="@style/SplashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Intro"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ResetPassword"
            android:parentActivityName=".Login"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".TypeOfUser"
            android:parentActivityName=".Login"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".Needy.NeedyRegister"
            android:parentActivityName=".Login"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".Volunteer.VolunteerRegister"
            android:parentActivityName=".TypeOfUser"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".Needy.NeedyMainPage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Volunteer.VolunteerMainPage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Needy.NeedyPasswordChange"
            android:launchMode="singleTask"
            android:parentActivityName=".Needy.NeedyMainPage"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".Needy.ScribeSearchPage"
            android:parentActivityName=".Needy.NeedyMainPage"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".Needy.DisplaySearchedVolunteers"
            android:parentActivityName=".Needy.ScribeSearchPage"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".Needy.DisplaySingleScribeDetails"
            android:parentActivityName=".Needy.DisplaySearchedVolunteers"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Needy.NeedyProfileEdit"
            android:parentActivityName=".Needy.NeedyMainPage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Volunteer.VolunteerProfileEdit"
            android:parentActivityName=".Volunteer.VolunteerMainPage"
            android:theme="@style/AppTheme.NoActionBar" />
