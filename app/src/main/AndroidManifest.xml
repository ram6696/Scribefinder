            android:name=".Volunteer.VolunteerPasswordChange"
            android:launchMode="singleTask"
            android:parentActivityName=".Volunteer.VolunteerMainPage"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".StudyMaterials.NotesBA$StudyMaterial"
