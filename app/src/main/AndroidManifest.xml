            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".StudyMaterials.NotesBA"
            android:parentActivityName=".StudyMaterials.NotesBA$StudyMaterial"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
