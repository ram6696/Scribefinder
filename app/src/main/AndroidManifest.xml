            android:parentActivityName=".Needy.NeedyMainPage"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
            android:name=".ViewUploadsActivity"
            android:parentActivityName=".Needy.NeedyMainPage"
            android:theme="@style/AppTheme.NoActionBar"
            tools:targetApi="jelly_bean" />
        <activity
